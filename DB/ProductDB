create table Product(
p_no number primary key,
pName varchar(120),
detailText varchar(6000),
pImage varchar(120),
sellerNick varchar(120),
seller varchar(120),
detailPageUrl varchar(120),
salePrice varchar(120),
delivery varchar(120),
buySatisfy varchar(120),
discount varchar(120),
mileage varchar(120),
Invntry varchar(120)
);

create table Product_img(
i_no number primary key,
p_no number(120),
imgAdr varchar(120)
);